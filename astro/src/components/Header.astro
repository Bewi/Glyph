---
import { getLangFromUrl } from "@i18n/utils";
const lang = getLangFromUrl(Astro.url);
---

<header>
  <nav aria-labelledby="mainmenu">
    <ul>
      <li>
        <a href={`/${lang}`}>Home</a>
      </li>
      <li>
        <a href={`/${lang}/content`}>Contents</a>
      </li>
    </ul>
  </nav>

  <a href="/fr">FR</a>
  <a href="/en">EN</a>
</header>
